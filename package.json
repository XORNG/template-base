{
  "name": "@xorng/template-base",
  "version": "0.1.0",
  "description": "Base template utilities for XORNG sub-agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "npx tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "xorng",
    "mcp",
    "sub-agent",
    "template",
    "ai"
  ],
  "author": "XORNG Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "pino": "^9.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@xorng/core": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@xorng/core": {
      "optional": true
    }
  }
}
